{
  "icon": "assets/images/icon.png",
  "name": "vscodelaraveldocs",
  "displayName": "Laravel Documentation",
  "description": "Laravel Documentation on Visual Studio Code",
  "publisher": "FranklinShera",
  "author": {
    "name": "Franlin Shera"
  },
  "license": "GPL-3.0",
  "version": "1.9.1",
  "homepage": "https://github.com/vfshera/laraveldocs-vscode",
  "bugs": "https://github.com/vfshera/laraveldocs-vscode/issues",
  "repository": {
    "url": "https://github.com/vfshera/laraveldocs-vscode",
    "type": "git"
  },
  "keywords": [
    "laravel docs",
    "laravel documentation",
    "laravel"
  ],
  "preview": true,
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:artisan"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "assets/images/laravel.svg",
          "id": "laraveldocs-sidebar",
          "title": "Laravel Documentation"
        }
      ]
    },
    "views": {
      "laraveldocs-sidebar": [
        {
          "id": "docs-list",
          "type": "webview",
          "name": "",
          "icon": "assets/images/laravel.svg"
        }
      ]
    }
  },
  "scripts": {
    "docs:generate": "node dist/generator/Generator.js",
    "vscode:prepublish": "npm run build:all",
    "compile": "tsc -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./dist/test/runTest.js",
    "watch": "concurrently \"cd webview-ui && npm run dev\" \"tsc -watch -p ./\"",
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run dev",
    "build:webview": "cd webview-ui && npm run build",
    "build:all": "npm run build:webview && npm run compile && npm run docs:generate"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "~20.8.2",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "glob": "^10.3.10",
    "highlight.js": "^11.8.0",
    "highlightjs-blade": "^0.1.0",
    "marked": "^9.1.0",
    "marked-highlight": "^2.0.6",
    "mocha": "^10.2.0",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.0"
  }
}
