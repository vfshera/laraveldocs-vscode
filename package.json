{
  "name": "laraveldocs",
  "displayName": "laraveldocs",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:laraveldocs.start",
    "onCommand:laraveldocs.refresh",
    "onView:docs-list"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laraveldocs.start",
        "title": "Show Panel",
        "category": "LaravelDocs"
      },
      {
        "command": "laraveldocs.refresh",
        "title": "Refresh",
        "category": "LaravelDocs"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "assets/images/fingerprint.svg",
          "id": "laraveldocs-sidebar",
          "title": "Laravel Docs"
        }
      ]
    },
    "views": {
      "laraveldocs-sidebar": [
        {
          "id": "docs-list",
          "type": "webview",
          "name": "",
          "icon": "assets/images//approval.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "concurrently": "^7.2.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "rollup": "^2.77.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.3.1",
    "tslib": "^2.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "marked": "^4.0.18"
  }
}
